{
    "name": "weaving-the-web/experiments",
    "description": "Driving experiments for fun's sake",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:thierrymarianne/twitteroauth.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:WeavingTheWeb/DoctrineExtensions.git"
        }
    ],
    "require": {
        "php": ">=7.2.9",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "auth0/auth0-php": "~5.0",
        "beberlei/DoctrineExtensions": "dev-master",
        "doctrine/inflector": "*",
        "doctrine/orm": "~2.4,>=2.4.5",
        "fabpot/goutte": "^3.2@dev",
        "fzaninotto/faker": "^1.9@dev",
        "guzzle/guzzle": "3.3.1",
        "incenteev/composer-parameter-handler": "~2.0",
        "kertz/twitteroauth": "dev-master",
        "kreait/firebase-php": "~4.18",
        "ocramius/proxy-manager": "2.2.3",
        "predis/predis": "^2.0@dev",
        "ramsey/uuid-doctrine": "dev-master",
        "sensio/framework-extra-bundle": "^5.5@dev",
        "symfony/console": "^5.1@dev",
        "symfony/flex": "^1.5@dev",
        "symfony/messenger": "^5.1@dev",
        "symfony/monolog-bundle": "^3.0@dev",
        "symfony/orm-pack": "dev-master",
        "symfony/security-bundle": "^5.1@dev",
        "symfony/translation": "^5.1@dev",
        "symfony/validator": "^5.1@dev",
        "thecodingmachine/safe": "^0.1.0@dev",
        "themattharris/tmhOAuth": "dev-master"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3@dev",
        "phpunit/phpunit": "7.1.5",
        "symfony/dotenv": "^5.1@dev",
        "symfony/maker-bundle": "^1.0@dev",
        "symfony/profiler-pack": "dev-master",
        "symfony/test-pack": "dev-master",
        "symfony/twig-pack": "dev-master"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4.2"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "app",
        "symfony-var-dir": "app",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}

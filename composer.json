{
    "name": "snapshots/worker",
    "license": "GPL-3.0-or-later",
    "description": "A worker collecting publications from online social network via Twitter API.",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "abraham/twitteroauth": "^4.0",
        "beberlei/assert": "3.3.0",
        "beberlei/doctrineextensions": "^1.3.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/inflector": "^2.0",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.8",
        "fabpot/goutte": "^4.0",
        "guzzlehttp/guzzle": "^7.4",
        "incenteev/composer-parameter-handler": "^2.0",
        "kreait/firebase-bundle": "^4.0",
        "ocramius/package-versions": "^1.9",
        "ocramius/proxy-manager": "^2.14",
        "oro/doctrine-extensions": "^2.0",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.8",
        "scienta/doctrine-json-functions": "^4.1",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^2.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/messenger": "^5.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/validator": "^5.4",
        "thecodingmachine/safe": "^2.2.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3@dev",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.1.6"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "app",
        "symfony-var-dir": "app",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}

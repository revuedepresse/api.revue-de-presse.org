[program:handle_news_messages]
command=/data/repositories/daily-press-review/bin/consume_twitter_api_for_news.sh
numprocs=4
numprocs_start=0
process_name=%(program_name)s_%(process_num)02d
priority=999
autostart=true
autorestart=true
startsecs=1
startretries=3
exitcodes=0,2
stopsignal=TERM
stopwaitsecs=10
killasgroup=false
user=weaver
redirect_stderr=false
stdout_logfile=/data/repositories/daily-press-review/var/logs/consume_twitter_api.out
stdout_logfile_maxbytes=250MB
stdout_logfile_backups=10
stderr_logfile=/data/repositories/daily-press-review/var/logs/consume_twitter_api.err
stderr_logfile_maxbytes=250MB
stderr_logfile_backups=10
environment=MAX_PROCESSES=4,MEMORY_LIMIT=128M,MESSAGES=10,PROJECT_DIR="/data/repositories/daily-press-review"
